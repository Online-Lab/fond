html
  font-family 'Tahoma', serif
body
  height auto
  vertical-align top
  background-color #ebe8e0
  font-size 10px
[layout=row]
  flex-direction row


  
  
ul,
figure,
figcaption
  display inline-block
  margin 0
  padding 0

li
  list-style-type none
  margin 0
  padding 0
  display inline-block
  


.browsehappy
  margin 0.2em 0
  background #ccc
  color #000
  padding 0.2em 0


.center
  display block
  margin auto
  width 960px


.clr:after
  content ""
  display block
  height 0
  clear both
  
.main-cantainer
  margin-top 10px




.sand-blind
  &.col1
    background-color white
    

  .main-slider
    display none
  .news-list-wrap
    .img-wrap
      display none
  


