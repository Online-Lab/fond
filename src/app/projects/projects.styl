.upper
    text-transform uppercase

.projects-wrap
    background-color #fff
    padding 10px
    border-radius 10px
.projects
    width 70%
    display inline-block
    height 550px
    object
        width 100%
        height 100%
    
.map a:hover 
    path:not(.map_bg)
        fill #debba5
    text
        fill blue
.projects-list
    display inline-block
    width 29%
    float right
    margin-top 20px
    li
        margin-bottom 3px
        font-size 1.5em
    a
        color #4e737c
        &:hover
            font-weight 700
a.hovered
    path
        fill #debba5
