.article-wrap .img-wrap
    width 30%
    height auto
    float left
    margin 0 20px 20px 0
    img
        display block
        width 100%
        
.article-wrap
    margin-bottom 40px
    &:after
        content ""
        display block
        height 0
        clear both
    

.region-name
    padding-bottom 10px
